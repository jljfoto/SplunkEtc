  <row>
    <panel>
      <title>Last message logged in Splunkd.log</title>
      <table>
        <search>
          <query>index=lab4 source=*splunkd.log starttimeu=1476120288.178
endtimeu=1476132966.639 | stats latest(_time) as lastMessage | eval lastMessage =
"Last message was logged in splunkd.log at: ".strftime(lastMessage,"%m/%d/%Y %H:%M:%S")</query>
          <earliest>0</earliest>
          <sampleRatio>1</sampleRatio>
        </search>
        <option name="count">20</option>
        <option name="dataOverlayMode">none</option>
